SET !TIMEOUT_STEP 100
URL GOTO=https://live.cloud.api.aig.com/life/connext-security/public/login?DocID=143
TAG POS=1 TYPE=INPUT:TEXT FORM=ID:frmLogin ATTR=NAME:username CONTENT={{usn}}
