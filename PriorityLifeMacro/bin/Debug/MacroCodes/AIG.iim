WAIT SECONDS=10
TAG POS=1 TYPE=BUTTON ATTR=TXT:Export
TAG POS=1 TYPE=SPAN ATTR=TXT:Excel
WAIT SECONDS=20
ONDOWNLOAD FOLDER={{downloadpath}}\AIG\{{!NOW:yyyymmdd}}\ FILE=* WAIT=YES
